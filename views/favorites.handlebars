          <h2>{{locations.0.dba_name}} - {{locations.0.address}}</h2>
          <div class="table-responsive" id="tableResults">
            <table class="table table-striped table-sm">
              <thead>
                <tr>
                  <th>Name</th>
                  <th>Inspection Date</th>
                  <th>Risk</th>
                  <th>Results</th>
                  <th>Violation</th>
                </tr>
              </thead>
              <tbody id="tableSearch">
                {{#each favorites}}
